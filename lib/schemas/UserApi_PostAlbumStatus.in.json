{
    "type": "object",
    "properties": {
        "albumId": {
            "required": true,
            "type": "integer",
            "format": "int32"
        },
        "collectionStatus": {
            "required": true,
            "type": "string",
            "enum": [
                "Nothing",
                "Wishlisted",
                "Ordered",
                "Owned"
            ]
        },
        "mediaType": {
            "required": true,
            "type": "string",
            "enum": [
                "PhysicalDisc",
                "DigitalDownload",
                "Other"
            ]
        },
        "rating": {
            "required": true,
            "type": "integer",
            "format": "int32"
        }
    }
}